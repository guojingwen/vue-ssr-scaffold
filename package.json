{
  "name": "vue-ssr-scaffold",
  "description": "A Vue-SSR-Mobile scaffold",
  "author": "dashu <2315162186@qq.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env node server",
    "debugger": "cross-env node --inspect server",
    "dev:unlint": "cross-env LINT_ENV=unlint node server",
    "start": "cross-env NODE_ENV=production node server",
    "start:pm2": "export NODE_ENV=production && pm2 delete ssr && pm2 start server.js --name ssr",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --hide-modules",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "pre-commit": "echo 'run eslint check...' && exit 0"
  },
  "pre-commit": [
    "pre-commit",
    "lint"
  ],
  "engines": {
    "node": ">=6.9",
    "npm": ">=4.0"
  },
  "dependencies": {
    "autosize": "^4.0.0",
    "axios": "^0.17.1",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.1",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "lodash.clonedeep": "^4.5.0",
    "lru-cache": "^4.1.1",
    "mint-ui": "^2.2.13",
    "morgan": "^1.9.0",
    "node-schedule": "^1.2.5",
    "raven-js": "^3.23.3",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.11",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.11",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-code-frame": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^1.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-modules": "^0.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.6",
    "file-stream-rotator": "^0.2.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.0.4",
    "moment": "^2.21.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "postcss-px2rem": "^0.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "rimraf": "^2.7.1",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.6.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.11",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.2",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  }
}
